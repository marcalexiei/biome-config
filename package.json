{
  "name": "@marcalexiei/biome-config",
  "version": "1.0.1",
  "description": "Custom BiomeJS config",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "biome",
    "config"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marcalexiei/biome-config"
  },
  "author": "Marco Pasqualetti @marcalexiei",
  "license": "MIT",
  "exports": "./biome-config.json.json",
  "files": [
    "biome-config.json"
  ],
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "format": "biome format",
    "format:fix": "biome format --fix",
    "test": "pnpm --workspace-concurrency=1 --filter=\"tests-*\" run test",
    "release": "changeset publish",
    "check-all": "pnpm run format && pnpm run test"
  },
  "peerDependencies": {
    "@biomejs/biome": "^2.0.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.29.5",
    "@types/node": "24.0.15"
  }
}
